{
  "name": "acorn-jsx-walk",
  "version": "1.1.0",
  "description": "JSX support for Acorn Walk",
  "keywords": [
    "acorn",
    "jsx",
    "walk",
    "parse"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sderosiaux/acorn-jsx-walk.git"
  },
  "main": "dist/jsxWalk.js",
  "module": "dist/jsxWalk.es.js",
  "jsnext:main": "dist/jsxWalk.es.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run bundle && npm run bundle:min",
    "bundle": "./node_modules/rollup/bin/rollup -c ./build/rollup.conf.js",
    "bundle:min": "./node_modules/rollup/bin/rollup -c ./build/rollup.conf.min.js",
    "lint": "./node_modules/eslint/bin/eslint.js src test",
    "pretest": "rimraf .reify-cache/ && npm run lint",
    "test": "mocha -r reify test/**/*.spec.js",
    "testing": "npm test -- --watch",
    "prepublish": "npm test && npm run build"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^5.13.0",
    "mocha": "^5.2.0",
    "reify": "^0.18.1",
    "rimraf": "^2.5.2",
    "rollup": "^1.1.2",
    "rollup-plugin-buble": "^0.19.6",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-uglify": "^6.0.2"
  },
  "author": "Julian Lloyd <tealight@jlmak.es>",
  "license": "MIT"
}
